<% include partials/header %>

<link rel="stylesheet" href="/css/centre.css">

<div class="carddiv container">
    <h2>Channelling Centres</h2>
    <% centres.forEach(function(centre){ %>
        <div class="card">
          <div class="card-body">
            <h5 class="card-title"><%=centre.name%></h5>
            <h6 class="card-subtitle"><%=centre.contact%></h6>
            <p class="card-text"><%=centre.address%></p>
          </div>
        </div>
    <% }); %>
</div>

<% include partials/footer %>